   <!--=============== Archotech powder ====================-->

   <ThingDef ParentName="ResourceBase">
    <defName>Spacer_Powder_weed</defName>
    <label>Archotech powder</label>
    <stackLimit>25</stackLimit>
    <description>Archotech powder. Extremely powerful, composed mostly of pure hydrogen, but somehow stable in room temperature in solid state</description>
    <graphicData>
      <texPath>Things/Powders/arch</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <recipeMaker>
      <skillRequirements>
        <Crafting>16</Crafting>
      </skillRequirements>
      <researchPrerequisite>spacerpowder</researchPrerequisite>
      <recipeUsers Inherit="False">
        <li>AmmoBench</li>
      </recipeUsers>
    </recipeMaker>
    <costList>
      <Chemfuel>350</Chemfuel>
      <ComponentIndustrial>5</ComponentIndustrial>
    </costList>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <PowderPower>4.5</PowderPower>
      <MarketValue>5000</MarketValue>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>0.5</Mass>
      <Flammability>3.2</Flammability>
      <Explodability>3</Explodability>
    </statBases>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
